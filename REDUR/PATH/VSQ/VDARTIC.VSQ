CURSOR=CDAMEARTICULO SELECT CODART,VD.DIVIDEXBUSQUEDA(1, DESART, ' ', 30, 0) DESART1,
                      VD.DIVIDEXBUSQUEDA(2, DESART, ' ', 30, 0) DESART2,CODNAC,NVL(CODEAN,'00000000000000') CODEAN,RTRIM(LTRIM(TO_CHAR(UNIEMB,'99999'))) UNIEMB
                FROM VDARTIC
                WHERE  CODART=:CODART;

CURSOR=CSELART SELECT AR.CODART,AR.DESART,AR.UNIEMB,AR.CODEAN,AR.CODNAC,LOT.CODLOT,LOT.CADUCI
        FROM VDARTIC AR,VDLOTES LOT
        WHERE LOT.CODART(+)=AR.CODART AND AR.CODART=:CODART AND CODLOT(+)=:CODLOT;