CURSOR=CSELKIT SELECT ART.CODART, CODEAN EANUNI, CODARTALT EANPAQ, ART.DESART FROM VDARTIC ART, VDTRADCODIGO TRC WHERE ART.CODART = TRC.CODART AND TRC.FORMATO = 'PAQ' AND ART.CODART=:CODART;

CURSOR=CSELORDKIT SELECT CODARTKIT,CODORDKIT,ART1_CODART,SUBSTR(ART1_DESART, 0, 25) ART1_DESART,ART1_CODEAN,TO_CHAR(ART1_CANTIDAD) ART1_CANTIDAD,ART1_UNIDADESHOSTCLI,ART2_CODART,SUBSTR(ART2_DESART, 0, 25) ART2_DESART,ART2_CODEAN,TO_CHAR(ART2_CANTIDAD) ART2_CANTIDAD,ART2_UNIDADESHOSTCLI,ART3_CODART,SUBSTR(ART3_DESART, 0, 25) ART3_DESART,ART3_CODEAN,TO_CHAR(ART3_CANTIDAD) ART3_CANTIDAD,ART3_UNIDADESHOSTCLI,TO_CHAR(ART4_CODART) ART4_CODART,SUBSTR(ART4_DESART, 0, 25) ART4_DESART,ART4_CODEAN,TO_CHAR(ART4_CANTIDAD) ART4_CANTIDAD,ART4_UNIDADESHOSTCLI,ART5_CODART,SUBSTR(ART5_DESART, 0, 25) ART5_DESART,ART5_CODEAN,TO_CHAR(ART5_CANTIDAD) ART5_CANTIDAD,ART5_UNIDADESHOSTCLI,ART6_CODART,SUBSTR(ART6_DESART, 0, 25) ART6_DESART,ART6_CODEAN,TO_CHAR(ART6_CANTIDAD) ART6_CANTIDAD,ART6_UNIDADESHOSTCLI,ART7_CODART,SUBSTR(ART7_DESART, 0, 25) ART7_DESART,ART7_CODEAN,TO_CHAR(ART7_CANTIDAD) ART7_CANTIDAD,ART7_UNIDADESHOSTCLI,ART8_CODART,SUBSTR(ART8_DESART, 0, 25) ART8_DESART,ART8_CODEAN,TO_CHAR(ART8_CANTIDAD) ART8_CANTIDAD,ART8_UNIDADESHOSTCLI,ART9_CODART,SUBSTR(ART9_DESART, 0, 25) ART9_DESART,ART9_CODEAN,TO_CHAR(ART9_CANTIDAD) ART9_CANTIDAD,ART9_UNIDADESHOSTCLI,ART10_CODART,SUBSTR(ART10_DESART, 0, 25) ART10_DESART,ART10_CODEAN,TO_CHAR(ART10_CANTIDAD) ART10_CANTIDAD,ART10_UNIDADESHOSTCLI 
 FROM (
 SELECT 'ART' || ROWNUM IDARTIC,  ART.CODART, ART.DESART, CODEAN, OKL.CANTIDAD / OKC.CANTIDAD CANTIDAD, OKL.UNIDADESHOSTCLI, OKC.CODART CODARTKIT, OKC.CODORDKIT 
 FROM VDARTIC ART, VDORDKITLIN OKL, VDORDKITCAB OKC WHERE ART.CODART = OKL.CODART AND OKC.CODORDKIT = OKL.CODORDKIT AND OKL.CODORDKIT = :CODORDKIT) ART
 PIVOT(MAX(CODART) CODART, MAX(DESART) DESART,MAX(CODEAN) CODEAN,MAX(CANTIDAD) CANTIDAD,MAX(UNIDADESHOSTCLI) UNIDADESHOSTCLI
       FOR IDARTIC IN ( 'ART1' ART1,'ART2' ART2,'ART3' ART3,'ART4' ART4, 'ART5' ART5, 'ART6' ART6, 'ART7' ART7,'ART8' ART8,'ART9' ART9,'ART10' ART10));