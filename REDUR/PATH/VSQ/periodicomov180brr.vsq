CURSOR=CREINTENTA  UPDATE VDMOVIM MOV 
                      SET STATUS=1900 
					WHERE STATUS=1950 
                      AND EXISTS (SELECT CODART FROM VDARTIC WHERE CODART=MOV.CODART AND CODDIV = 'BRR');                      
                     