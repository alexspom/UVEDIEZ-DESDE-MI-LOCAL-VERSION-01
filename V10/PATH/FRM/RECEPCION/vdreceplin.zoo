1. CONSULTA DEL ARTICULO,FEJECUTAFORM("VDARTIC","S","CODART=:CODART")
2. STOCK DEL ARTICULO,FEJECUTAFORM("VDSTKALM","N","","CODART=:CODART","","CONSULTA DEL STOCK DEL ARTICULO :CODART","S")
3. CONSULTA DEL LOTE,FEJECUTAFORM("VDLOTES","S","CODART=:CODART AND CODLOT=:CODLOT")
4. STOCK DEL LOTE,FEJECUTAFORM("VDSTKLOT","N","CODART=:CODART AND CODLOT=:CODLOT","","","CONSULTA DEL STOCK DEL ARTICULO :CODART LOTE :CODLOT","S")
5. MOVIMIENTOS DE ENTRADA DE LA RECEPCION,FEJECUTAFORM("VDMOVIM","S","CODMOV IN (SELECT CODMOV FROM VDMOVIM WHERE CODRECEP = :CODRECEP AND STATUS < VDST.FMOVANULADO AND CODUBIORI LIKE 'FUENTE%')")
6. MOVIMIENTOS DE LA RECEPCION,FEJECUTAFORM("VDMOVIM","S","CODMOV IN (SELECT CODMOV FROM VDMOVIM WHERE CODART=:CODART AND CODLOT=:CODLOT AND CODRECEP = :CODRECEP AND STATUS < VDST.FMOVANULADO)")
