NOMBRE=TICK;

PROCEDURE=INICIALIZAZONAS,BEGIN UPDATE VDZONAS SET NEXTTICK=(VD.FECHASEC+((:VDINFODISP.NEXTTICK)/86400)) WHERE IDINFOGEST=:VDZONAS.IDINFOGEST;END;@
PROCEDURE=ACTUALIZAZONA,BEGIN UPDATE VDZONAS SET NEXTTICK=(VD.FECHASEC+((:VDINFODISP.NEXTTICK)/86400)) WHERE CODZONA=:VDZONAS.CODZONA;END;@
PROCEDURE=ACTUALIZADISP,BEGIN UPDATE VDINFODISP SET NEXTTICK=(VD.FECHASEC+((:VDINFODISP.NEXTTICK)/86400)) WHERE IDINFOPICK=:VDINFODISP.IDINFOPICK AND NUMDISP=:VDINFODISP.NUMDISP;END;@
PROCEDURE=RESETEAZONA,BEGIN UPDATE VDZONAS SET NEXTTICK=NULL WHERE CODZONA=:VDZONAS.CODZONA;END;@
PROCEDURE=RESETEADISP,BEGIN UPDATE VDINFODISP SET NEXTTICK=NULL WHERE IDINFOPICK=:VDINFODISP.IDINFOPICK AND NUMDISP=:VDINFODISP.NUMDISP;END;@
